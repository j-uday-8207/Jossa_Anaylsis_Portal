{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/final.csv",
      "dest": "/build/final.csv"
    },
    {
      "src": "/output.json",
      "dest": "/build/output.json"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}